#include <cassert>
using namespace std;

struct expression
{

};

struct number : expression
{
	number(double value) : value_(value) {}
		double value() const { return value_; }
		double evaluate() const { return value_; }
private:
	double value_;
};
struct binaryOperations : expression
{
	enum
	{
		plus = '+';
	minus=''
	};
};
